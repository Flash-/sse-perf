GET   /                      controllers.Application.index

GET   /metricsFeed           controllers.Application.metricsFeed
GET   /clients/add           controllers.Application.addClients(n: Int, url: String)
GET   /clients/remove        controllers.Application.removeClients(n: Int)
GET   /clients/removeAll     controllers.Application.removeAllClients

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file        controllers.Assets.at(path="/public", file)
